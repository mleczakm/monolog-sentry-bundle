{
  "name": "dziki/monolog-sentry-bundle",
  "type": "symfony-bundle",
  "description": "Improve logging to Sentry with user, user agent, revision and Symfony version data",
  "keywords": [
    "sentry",
    "logging",
    "logger",
    "monolog",
    "symfony",
    "bundle"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "MichaÅ‚ Mleczko",
      "email": "michal@mleczko.waw.pl"
    }
  ],
  "require": {
    "php": ">=7.1",
    "psr/simple-cache": "^1.0",
    "psr/simple-cache-implementation": "^1.0",
    "symfony/dependency-injection": "^4.2|^5.0",
    "symfony/monolog-bundle": "^3.3",
    "monolog/monolog": "^1",
    "symfony/monolog-bridge": "^4.2|^5.0",
    "sentry/sentry": "^1.9.1"
  },
  "conflict": {
    "donatj/phpuseragentparser": "<0.11"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^4.1.8",
    "friendsofphp/php-cs-fixer": "^2.12",
    "nyholm/symfony-bundle-test": "^1.4",
    "matthiasnoback/symfony-dependency-injection-test": "dev-master",
    "donatj/phpuseragentparser": "^0.11",
    "symfony/security-bundle": "^4.2|^5.0",
    "symfony/string": "^5.0"
  },
  "suggest": {
    "donatj/phpuseragentparser": "Basic package for parsing User-Agent header, faster than native browscap",
    "symfony/security-bundle": "Required for current user data appending to logs"
  },
  "autoload": {
    "psr-4": {
      "Dziki\\MonologSentryBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": { "": "Tests/" }
  }
}
